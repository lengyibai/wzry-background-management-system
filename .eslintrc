{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended",
    "@vue/prettier"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/no-multiple-template-root": "warn",
    "no-unused-vars": "warn",
    "vue/no-unused-components": "warn",
    "vue/no-mutating-props": "warn",
    "vue/multi-word-component-names": "off",
    "vue/no-parsing-error": "warn"
  }
}