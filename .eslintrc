{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended",
    "@vue/prettier"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/no-multiple-template-root": "off",
    "no-unused-vars": "warn",
    "vue/no-mutating-props": "off",
    "vue/multi-word-component-names":"off",
    "vue/no-parsing-error":"off"
  }
}
